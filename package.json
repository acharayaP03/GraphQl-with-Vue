{
  "name": "graphqlvue",
  "version": "1.0.0",
  "description": "graphQl apollo serve with vue on a client side ",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "cd client && npm run serve",
    "dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\""
  },
  "keywords": [
    "vuejs",
    "graphql",
    "fullstack",
    "development",
    "with",
    "graphql",
    "and",
    "vue"
  ],
  "author": "Trishten ac",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^3.6.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^11.0.0",
    "graphql": "^16.2.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mongoose": "^6.1.6"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
